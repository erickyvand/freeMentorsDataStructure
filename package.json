    "@babel/register": "^7.5.5",
    "@hapi/joi": "^15.1.1",
    "bcrypt": "^3.0.6",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "coveralls": "^3.0.6",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mocha": "^6.2.0",
    "nyc": "^14.1.1"
  },
  "devDependencies": {
